<!-- Sections(MEGUA) or Category(Moodle)-->
<question type="category">
<category>
<text>$course$/{{sections}}/{{problemname}}</text>
</category>
</question>
<!-- {{exercisename}} ekey={{ekey}} -->
<question type="cloze">
  <name>
  <text>{{problemname}} ({{ekey}})</text>
  </name>
  <questiontext format="html">
  <text><![CDATA[{{problemtext}}]]></text>
  </questiontext>
  <generalfeedback format="html">
  <text><![CDATA[{{answertext}}]]></text>
  </generalfeedback>
  <shuffleanswers>true</shuffleanswers>
</question>

